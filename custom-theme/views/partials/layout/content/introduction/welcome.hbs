<div id='welcome' class='doc-row'>
  <div class='doc-copy'>
    {{md info.description}}
    {{md info.linkToAPIDocs}}
    {{md info.rateLimits.title}}
    <div>
      <h1>GraphQL Rate Limits</h1>
      {{! limitations }}
      <p>{{info.rateLimits.limitations.title}}</p>
      <ul>
        <li>{{info.rateLimits.limitations.limit1}}</li>
        <li>{{info.rateLimits.limitations.limit2}}</li>
        <li>{{info.rateLimits.limitations.limit3}}</li>
      </ul>
      <p>{{info.rateLimits.contactMessage}}</p>
      <br />
      {{! calculating query complexity }}
      <h3>{{info.rateLimits.complexity.title}}</h3>
      <p>{{info.rateLimits.complexity.block1.heading}}</p>
      <pre style='color: black;'><code>{{info.rateLimits.complexity.block1.snippet}}</code></pre>
      <p>{{info.rateLimits.complexity.block2.heading}}</p>
      <pre style='color: black;'><code>{{info.rateLimits.complexity.block2.snippet}}</code></pre>
      <h3>{{info.rateLimits.additionalInformation.title}}</h3>
      <p>{{info.rateLimits.additionalInformation.information.title}}</p>
      <ul>
        <li>{{info.rateLimits.additionalInformation.information.header1}}</li>
        <li>{{info.rateLimits.additionalInformation.information.header2}}</li>
        <li>{{info.rateLimits.additionalInformation.information.header3}}</li>
      </ul>
    </div>
  </div>
  <div class='doc-examples'>
    {{#if info.contact}}
      <div class='example-section welcome-contact-section'>
        <h5>Contact</h5>
        {{#if info.contact.name}}
          <p class='contact-name'>{{info.contact.name}}</p>
        {{/if}}
        {{#if info.contact.email}}
          <p class='contact-email'><a
              href='mailto:{{info.contact.email}}'
            >{{info.contact.email}}</a></p>
        {{/if}}
        {{#if info.contact.url}}
          <p class='contact-url'><a href='{{info.contact.url}}'>{{info.contact.url}}</a></p>
        {{/if}}
      </div>
    {{/if}}

    {{#if info.license}}
      <div class='example-section welcome-license-section'>
        <h5>License</h5>
        {{#if info.license.name}}
          <p class='license-name'>{{info.license.name}}</p>
        {{/if}}
        {{#if info.license.url}}
          <p class='license-url'><a href='{{info.license.url}}'>{{info.license.url}}</a></p>
        {{/if}}
      </div>
    {{/if}}

    {{#if info.termsOfService}}
      <div class='example-section welcome-tos-section'>
        <h5>Terms of Service</h5>
        <p><a href='{{info.termsOfService}}'>{{info.termsOfService}}</a></p>
      </div>
    {{/if}}

    {{#if url}}
      <div class='example-section welcome-api-endpoints-section example-section-is-code'>
        <h5>API Endpoints</h5>
        <pre><code>{{url}}</code></pre>
      </div>
    {{/if}}

    {{#if headers}}
      <div class='example-section welcome-api-endpoints-section example-section-is-code'>
        <h5>Headers</h5>
        <pre><code>{{headers}}</code></pre>
      </div>
    {{/if}}

    {{#if info.version}}
      <div class='example-section welcome-version-section'>
        <h5>Version</h5>
        <p>{{info.version}}</p>
      </div>
    {{/if}}
  </div>
</div>